config prometheus-node-push-influx 'main'
	option disabled '1'
	option server_address 'victoriametrics.server'
	option server_port '8189' # need to run the VictoriaMetrics server with -influxListenAddr :8189
	option interval '30'

config prometheus-node-push-influx 'backup'
	option disabled '1'
	option server_address 'victoriametrics2.server'
	option server_port '8189' # need to run the VictoriaMetrics server with -influxListenAddr :8189
	option interval '30'
